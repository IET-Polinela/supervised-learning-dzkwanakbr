arget (Y)\n","X = df_encoded.drop(columns=[\"SalePrice\", \"Id\"])  # Hapus \"Id\" karena bukan fitur penting\n","Y = df_encoded[\"SalePrice\"]\n","\n","# 3Ô∏è‚É£ Membagi dataset menjadi training dan testing (80:20)\n","X_train, X_test, Y_train, Y_test = train_test_split(X, Y, test_size=0.2, random_state=42)\n","\n","# 4Ô∏è‚É£ Gabungkan kembali training & testing ke dalam satu dataset dengan label \"Train\" dan \"Test\"\n","df_encoded[\"DataType\"] = \"Train\"\n","df_encoded.loc[X_test.index, \"DataType\"] = \"Test\"\n","\n","# 5Ô∏è‚É£ Simpan dataset hasil preprocessing\n","df_encoded.to_csv(\"data_encode.csv\", index=False)\n","\n","# üîç Menampilkan informasi dataset setelah preprocessing\n","print(\"üìå Dataset setelah preprocessing disimpan sebagai 'data_encode.csv'\")\n","print(f\"Total data: {df_encoded.shape}\")\n","print(f\"Fitur numerik: {len(numerical_features)} | Fitur kategori: {len(categorical_features)}\")\n","print(f\"Training Data: {X_train.shape} | Testing Data: {X_test.shape}\")\n","\n","# Tampilkan beberapa data pertama\n","print(\"\\nüìä Contoh data setelah preprocessing:\")\n","print(df_encoded.head())"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Ttl7_xTQ3sEi","executionInfo":{"status":"ok","timestamp":1742894697707,"user_tz":-420,"elapsed":54,"user":{"displayName":"Dzakwan Akbar","userId":"10974690383539103573"}},"outputId":"911c1cd4-2ec5-4986-90dd-6156dd2438dc"},"execution_count":9,"outputs":[{"output_type":"stream","name":"stdout","text":["Overwriting datapreprocessing.py\n"]}]},{"cell_type":"code","source":["!python datapreprocessing.py"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"7X_q30cs33jz","executionInfo":{"status":"ok","timestamp":1742894705288,"user_tz":-420,"elapsed":4473,"user":{"displayName":"Dzakwan Akbar","userId":"10974690383539103573"}},"outputId":"2c64ce8a-5e4d-4e1c-9228-263c4dd00961"},"execution_count":10,"outputs":[{"output_type":"stream","name":"stdout","text":["üìå Dataset se